{
    "name": "greenman/trait-injection-bundle",
    "type": "symfony-bundle",
    "description": "Symfony bundle for trait injection.",
    "keywords": ["greenman", "trait", "injection", "symfony", "bundle"],
    "homepage": "https://github.com/fermio/FermioTraitInjectionBundle",
    "license": "MIT",
    "authors": [{
        "name": "Diego Lewin",
        "email": "diego.lewin@gmail.com",
        "homepage": "http://ferm.io/"
    }],
    "require": {
        "php": "~5.4",
        "symfony/framework-bundle": "~2.1"
    },
    "require-dev": {
        "doctrine/common": "~2.2",
        "symfony/form": "~2.1",
        "symfony/security": "~2.1",
        "swiftmailer/swiftmailer": "~4.2",
        "symfony/validator": "~2.1"
    },
    "suggest": {
        "greenman/traits": "~1.0@dev"
    },
    "autoload": {
        "psr-0": {
            "Greenman\\Bundle\\TraitInjectionBundle\\": ""
        }
    },
    "target-dir": "Greenman/Bundle/TraitInjectionBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
